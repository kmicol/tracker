 <div class="container">
    <div class="row">
       <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">


            {{#each model as |post|}}
              <article class="post">
                <h2>{{post.title}}</h2>
                <div class="subtitle">
                  <h3>{{post.subtitle}}</h3>
                </div>
                
                <div class="author">
                  <span>Written by:</span> {{post.author}}
                </div>

              </article>
            {{/each}}

          </div>
        </div>
      </div>